<?xml version="1.0"?>
<ruleset>

    <file>./src/</file>
    <file>./tests/</file>

    <arg value="sp"/>
    <arg name="colors"/>
    <config name="testVersion" value="7.4-"/>

    <rule ref="Inpsyde"/>

    <rule ref="Inpsyde.CodeQuality.Psr4">
        <properties>
            <property
                name="psr4"
                type="array"
                value="Inpsyde=>src,Inpsyde\Tests=>tests"/>
        </properties>
    </rule>
</ruleset>
